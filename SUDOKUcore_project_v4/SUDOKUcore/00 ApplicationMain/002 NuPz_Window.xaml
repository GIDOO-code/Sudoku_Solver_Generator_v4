<Window x:Name="GNPXwin" x:Class="GNPXcore.NuPz_Win"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:extToolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit/extended" 
        xmlns:my="http://schemas.microsoft.com/wpf/2008/toolkit" 
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"  
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"

        xmlns:properties="clr-namespace:GNPXcore.Properties"
        xmlns:app="clr-namespace:GNPXcore"
     
        xmlns:GIDOO_space="clr-namespace:GIDOO_space"     
        
        mc:Ignorable="d"
        
        Title="NuPz_Window" 
        Loaded="Window_Loaded" Background="#FF181846" ShowInTaskbar="False" WindowStyle="None" 
        ShowActivated="True" ResizeMode="NoResize" Unloaded="Window_Unloaded" BorderBrush="Black" 
        MouseDown="Window_MouseDown" Width="972" Height="513" LocationChanged="GNPXwin_LocationChanged"
        WindowStartupLocation="Manual" >

    <!--
        ** For simple version:
        **   For consistency with the regular version, there are some unnecessary but unchanged parts.
    -->


    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Styles/DataGridStyle.xaml"></ResourceDictionary>
                <!-- <ResourceDictionary Source="/Styles/ComboBoxStyle.xaml"></ResourceDictionary> -->
                <ResourceDictionary Source="/Styles/ListBoxStyle1.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Styles/TabControlStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Styles/TextBlockStyle.xaml"></ResourceDictionary>

                <ResourceDictionary>
                    <Style TargetType="{x:Type ComboBox}">
                        <Style.Triggers>
                            <Trigger Property="IsKeyboardFocusWithin" Value="true">
                                <Setter Property="Background" Value="Navy"/>
                            </Trigger>
                        </Style.Triggers>
                    </Style>
                </ResourceDictionary>

            </ResourceDictionary.MergedDictionaries>

        </ResourceDictionary>
    </Window.Resources>

    <Grid VerticalAlignment="Stretch" PreviewKeyDown="Grid_PreviewKeyDown" SnapsToDevicePixels="True" ToolTip="Drag to move">
        <Grid.Background>
            <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
                <GradientStop Color="#FF121317" />
                <!-- #FF302A3E -->
                <GradientStop Color="#FF4C4C6F" Offset="1" />
                <GradientStop Color="#BE141225" Offset="0.432"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Label x:Name="GNPXGNPX" Content="GNPXv4 2023" FontFamily="MS Mincho" FontSize="36" Foreground="#FFFEFFFF" HorizontalAlignment="Left" 
               Margin="2,14,0,0" VerticalAlignment="Top" Height="41" FontWeight="Bold" MouseDoubleClick="GNPXGNPX_MouseDoubleClick" />
        <Button Content="Exit" FontFamily="MS Gothic" FontSize="12" Foreground="White" Height="29" HorizontalAlignment="Left" Margin="841,474,0,0" x:Name="appExit" VerticalAlignment="Top" Width="56" Click="appExit_Click" Style="{Binding}">
            <Button.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0.8" />
                    <GradientStop Color="#FF7F7E85" Offset="0.05" />
                    <GradientStop Color="#EA575757" Offset="0.55" />
                </LinearGradientBrush>
            </Button.Background>
        </Button>
        <Label Content="lblShortMessage" HorizontalAlignment="Left" x:Name="lblShortMessage" VerticalAlignment="Top" Foreground="Red" Visibility="Visible" Panel.ZIndex="6" Margin="800,10,0,0" FontFamily="MS Gothic" Background="{x:Null}" FontSize="16" />
        <RepeatButton  x:Name="btnProbPre"
                       Content="{Binding Resources.btnProbPre, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
                       Height="23" HorizontalAlignment="Left" Margin="281,30,0,0" VerticalAlignment="Top" Width="37" 
    		           Foreground="White" Click="btnProbPre_Click" BorderBrush="#FF2E9BDE" IsEnabled="False" >
            <RepeatButton.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0.8" />
                    <GradientStop Color="#FF7F7E85" Offset="0.05" />
                    <GradientStop Color="#EA575757" Offset="0.55" />
                </LinearGradientBrush>
            </RepeatButton.Background>
        </RepeatButton>
        <RepeatButton x:Name="btnProbNxt"
                      Content="{Binding Resources.btnProbNxt, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
                      Height="23" HorizontalAlignment="Left" Margin="319,30,0,0" 
    		VerticalAlignment="Top" Width="37" Foreground="White" Click="btnProbNxt_Click" BorderBrush="#FF2E9BDE" IsEnabled="False" >
            <RepeatButton.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0.8" />
                    <GradientStop Color="#FF7F7E85" Offset="0.05" />
                    <GradientStop Color="#EA575757" Offset="0.55" />
                </LinearGradientBrush>
            </RepeatButton.Background>
        </RepeatButton>
        <Label x:Name="lblPuzzle" 
               Content="{Binding Resources.lblPuzzle, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
               FontFamily="MS Gothic" FontSize="12" Foreground="#FFFEFFFF" HorizontalAlignment="Right" Margin="0,59,909,0" 
               VerticalAlignment="Top"  />
        <TextBox x:Name="txtProbNo" Background="#FF0A0A24" FontFamily="MS Gothic" FontSize="14" Foreground="White" Height="19" HorizontalAlignment="Left" Margin="61,60,0,0" Text="999" TextAlignment="Center" VerticalAlignment="Top" Width="50" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" />
        <TextBox x:Name="txtProbName" Text="........." Background="#FF0A0A24" Width="239" Margin="116,60,0,0" 
                 FontFamily="MS Gothic" FontSize="14" Foreground="White" Height="19" 
                 HorizontalAlignment="Left"   VerticalAlignment="Top" VerticalContentAlignment="Center" 
                 TextChanged="txtProbName_TextChanged" ToolTip="Problem name, optionally rewritable" />

        <!-- DataContext="{Binding}" -->
        <Image x:Name="PB_GBoard" Width="338" Height="338" Margin="18,108,0,0" Stretch="Fill" VerticalAlignment="Top"
                 HorizontalAlignment="Left" 
               Source="/00 ApplicationMain/images/LockedSet3D.png" MouseEnter="PB_GBoard_MouseEnter" MouseLeave="PB_GBoard_MouseLeave" ToolTip="copy / paste is possible" />
        <Grid x:Name="gridOverlap" HorizontalAlignment="Left" Margin="50,100,0,50" VerticalAlignment="Bottom">
            <Ellipse HorizontalAlignment="Left" Height="8" Margin="2,2,0,0" Stroke="DarkOrange" VerticalAlignment="Center" Width="8" Fill="#FFED1A1A" Panel.ZIndex="10"/>
            <Label Content="overlap" FontFamily="MS Gothic" Foreground="White" Margin="8,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        </Grid>

        <!--
               Source="/GNPXEx;component/10 Graphics/11 WPFColorPickerLib/Images/ColorSwatchCircle.png" MouseEnter="PB_GBoard_MouseEnter" MouseLeave="PB_GBoard_MouseLeave" ToolTip="copy / paste is possible" />
        
                MouseLeftButtonDown="PB_GBoard_MouseLeftButtonDown"
               MouseLeftButtonUp="PB_GBoard_MouseLeftButtonUp"
        -->

        <Image Height="36" HorizontalAlignment="Left" Margin="17,107,0,0" x:Name="GnumericPad" Stretch="Fill" 
               VerticalAlignment="Top" Width="36" Visibility="Visible" MouseMove="GnumericPad_MouseMove" 
               MouseLeave="GnumericPad_MouseLeave" MouseLeftButtonUp="GnumericPad_MouseLeftButtonUp" />
        <Label x:Name="lblCellsPuzzle"
            Content="{Binding Resources.lblCellsPuzzle, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
            FontFamily="MS Gothic" Foreground="White" HorizontalAlignment="Right" Margin="0,87,907,0"  VerticalAlignment="Top" Height="23" Background="{x:Null}" Width="50" HorizontalContentAlignment="Right" />
        <TextBlock x:Name="txbPuzzle" HorizontalAlignment="Left" Margin="62,92,0,0" TextWrapping="Wrap" Text="00" VerticalAlignment="Top" Width="22" Foreground="#FFC7C5F3" FontFamily="MS Gothic" FontSize="12" TextAlignment="Center" Background="#FF17171B"/>
        <Label x:Name="lblCellsSolved"
            Content="{Binding Resources.lblCellsSolved, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
            FontFamily="MS Gothic" Foreground="White" HorizontalAlignment="Right" Margin="0,87,835,0"  VerticalAlignment="Top" Height="23" Background="{x:Null}" Width="50" HorizontalContentAlignment="Right" />
        <TextBlock x:Name="txbSolved" HorizontalAlignment="Left" Margin="134,92,0,0" TextWrapping="Wrap" Text="00" VerticalAlignment="Top" Width="22" Foreground="#FFC7C5F3" FontFamily="MS Gothic" FontSize="12" TextAlignment="Center" Background="#FF17171B"/>
        <Label x:Name="lvlCellsUnsolved"
            Content="{Binding Resources.lvlCellsUnsolved, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
            FontFamily="MS Gothic" Foreground="White" HorizontalAlignment="Right" Margin="0,87,764,0"  VerticalAlignment="Top" Height="23" Background="{x:Null}" Width="50" HorizontalContentAlignment="Right" />
        <TextBlock x:Name="txbUnsolved" HorizontalAlignment="Left" Margin="204,92,0,0" TextWrapping="Wrap" Text="00" VerticalAlignment="Top" Width="22" Foreground="#FFC7C5F3" FontFamily="MS Gothic" FontSize="12" TextAlignment="Center" Background="#FF17171B"/>
        <Label x:Name="label1" Margin="0,87,651,0"  VerticalAlignment="Top" Height="23"
               Content="{Binding Resources.lblDifficulty, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
               FontFamily="MS Gothic" Foreground="White" HorizontalAlignment="Right"  Background="{x:Null}" FontSize="12" />
        <TextBox Background="#FF0A0A24" FontFamily="MS Gothic" FontSize="14" Foreground="White" Height="19" HorizontalAlignment="Left" Margin="318,89,0,0" x:Name="nUDDifficultyLevel" Text="0" TextAlignment="Center" VerticalAlignment="Top" Width="37" VerticalContentAlignment="Center" />
        <TabControl x:Name="tabCtrlMode" Background="Black" 
    		Height="445" HorizontalAlignment="Left" Margin="380,23,0,0"
    		VerticalAlignment="Top" Width="573" SelectionChanged="tabCtrlMode_SelectionChanged">
            <TabItem x:Name="tabAFile"              
                     Header="{Binding Resources.tabAFile, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
                     Style="{DynamicResource ResourceKey=styleTabItemTop}" 
                     FontSize="11" FontFamily="Microsoft NeoGothic" Height="22" VerticalAlignment="Top" Width="60" TabIndex="1" >
                <Grid x:Name="grdFileIO" Margin="0,0,0,0" Width="568" >
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <Grid.Background>
                        <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
                            <GradientStop Color="#FF3A3948" Offset="0" />
                            <GradientStop Color="#FF65646F" Offset="1" />
                        </LinearGradientBrush>
                    </Grid.Background>
                    <Button x:Name="btnOpenPuzzleFile"
                            Content="{Binding Resources.btnOpenPuzzleFile, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
                            Width="100" Height="25" VerticalAlignment="Top" Margin="42,31,0,0"
                		Foreground="White" HorizontalAlignment="Left" FontSize="12" Click="btnOpenPuzzleFile_Click" >
                        <Button.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFA3A3A3" Offset="0" />
                                <GradientStop Color="Black" Offset="1" />
                            </LinearGradientBrush>
                        </Button.Background>
                    </Button>
                    <TextBox Height="39" HorizontalAlignment="Left" Margin="52,61,0,0" x:Name="txtFileName" Text="Puzzle File" VerticalAlignment="Top" Width="299" TextWrapping="WrapWithOverflow" FontSize="12" Background="#FFCACEE2" Foreground="#FF0B0E2B" />
                    <CheckBox x:Name="chbInitialState" 
                              Content="{Binding Resources.chbInitialState, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
                              Foreground="White" Height="23" IsChecked="True" Margin="57,99,341,0" VerticalAlignment="Top" FontSize="12" VerticalContentAlignment="Center" />
                    <Button x:Name="btnSavePuzzle" Click="btnSavePuzzle_Click"
                            Content="{Binding Resources.btnSavePuzzle, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
                            Foreground="White" Height="25" HorizontalAlignment="Left" Margin="42,156,0,0" VerticalAlignment="Top" Width="100"  FontSize="12">
                        <Button.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFB0B0CA" Offset="0" />
                                <GradientStop Color="#FF0D0D1A" Offset="1" />
                                <GradientStop Color="#FF434399" Offset="0.508" />
                            </LinearGradientBrush>
                        </Button.Background>
                    </Button>
                    <Label Content="{Binding Resources.lblSaveAll, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
                           Foreground="White" Margin="44,179,0,0" Height="28" VerticalAlignment="Top" HorizontalAlignment="Left" Width="94" FontSize="12" />
                    <CheckBox x:Name="chbAdditionalSave" 
                              Content="{Binding Resources.chbAdditionalSave, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"                             
                              Foreground="White" Height="20" Margin="54,200,0,0"  VerticalAlignment="Top" IsChecked="True" HorizontalAlignment="Left" Width="201" FontSize="12" VerticalContentAlignment="Center" />
                    <CheckBox x:Name="chbFile81Nocsv"
                              Content="{Binding Resources.chbFormatLine, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"                             
                              Foreground="White" Margin="54,214,0,0" IsChecked="True" VerticalAlignment="Top" FontSize="12" VerticalContentAlignment="Center" Height="23" Width="185" HorizontalAlignment="Left" Checked="chbFile81Nocsv_Checked" Unchecked="chbFile81Nocsv_Checked" />
                    <CheckBox x:Name="chbFile81Plus"                             
                        Foreground="White" Margin="66,234,0,0" IsChecked="False" VerticalAlignment="Top" FontSize="12" VerticalContentAlignment="Center" Width="185" HorizontalAlignment="Left" Content="+ Blank" FontFamily="MS Gothic" IsEnabled="{Binding IsChecked, ElementName=chbFile81Nocsv}" />
                    <CheckBox x:Name="chbSolutionSort"
                              Content="{Binding Resources.chbSortByDiffOrder, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"                             
                              Foreground="White" Height="16" Margin="54,250,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" Width="176" FontSize="12" VerticalContentAlignment="Bottom" />
                    <CheckBox x:Name="cbxProbSolSetOutput" 
                              Content="{Binding Resources.chbSaveWithAnswer, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"                  
                              Foreground="White" Margin="54,267,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" Width="153" Checked="cbxProbSolSetOutput_Checked" Unchecked="cbxProbSolSetOutput_Checked" FontSize="12" VerticalContentAlignment="Bottom" />
                    <CheckBox x:Name="chbAddAlgorithmList"
                              Content="{Binding Resources.chbAddAlgorithmList, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
                              Foreground="Gray" Height="17" HorizontalAlignment="Left" Margin="66,281,0,0" VerticalAlignment="Top" Width="150" FontSize="12" VerticalContentAlignment="Bottom" IsEnabled="False" />
                    <Button x:Name="btnSaveToFavorites" Width="119"  Height="25" Margin="264,156,0,0" HorizontalAlignment="Left"  VerticalAlignment="Top" 
                            Content="{Binding Resources.btnSaveToFavorites, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"           
                            Foreground="White" FontSize="12"
                            Click="btnSaveToFavorites_Click" >
                        <Button.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFCACACA" Offset="0" />
                                <GradientStop Color="#FF6F6F85" Offset="1" />
                                <GradientStop Color="#FF4C31A0" Offset="0.508" />
                            </LinearGradientBrush>
                        </Button.Background>
                    </Button>
                    <Label Content="{Binding Resources.lblSaveCurrentPuzzle, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"           
                           Foreground="White" Height="28" HorizontalAlignment="Left" Margin="266,179,0,0" VerticalAlignment="Top" FontSize="12" Width="138" />
                </Grid>
            </TabItem>

            <TabItem x:Name="tabASolve" 
                     Header="{Binding Resources.tabASolve, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"
                     Style="{DynamicResource ResourceKey=styleTabItemTop}" FontFamily="MS Mincho" FontSize="12" TabIndex="2" Height="24" VerticalAlignment="Top">
                <Grid HorizontalAlignment="Left" Width="570">
                    <Grid.Background>
                        <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
                            <GradientStop Color="#FF3A3948" Offset="0" />
                            <GradientStop Color="#FF3B3858" Offset="1" />
                        </LinearGradientBrush>
                    </Grid.Background>
                    <TabControl Height="396" HorizontalAlignment="Left" Margin="10,6,0,0" x:Name="tabSolver" VerticalAlignment="Top" Width="530"
                                Background="Black" SelectionChanged="tabSolver_SelectionChanged" Grid.ColumnSpan="2">
                        <TabItem x:Name="TC_SDKAnalyzer" 
                                 Header="{Binding Resources.tabBSolve, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"                                              
                                 Style="{DynamicResource ResourceKey=styleTabItemTopSmall}" FontFamily="MS Mincho" FontSize="12" Uid="&quot;10&quot;">
                            <Grid x:Name="AutoSolNoDisp" Width="526" Margin="0,0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="30*"/>
                                    <ColumnDefinition Width="381*"/>
                                    <ColumnDefinition Width="115*"/>
                                </Grid.ColumnDefinitions>
                                <Grid.Background>
                                    <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
                                        <GradientStop Color="#FF3A3948" Offset="0" />
                                        <GradientStop Color="#FF65646F" Offset="1" />
                                    </LinearGradientBrush>
                                </Grid.Background>
                                <Label x:Name="lblStep"
                                       Content="{Binding Resources.lblStep, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"                                              
                                       FontSize="12" Foreground="White" Height="23" HorizontalAlignment="Right" Margin="0,14,54,0" VerticalAlignment="Top" Grid.Column="2" Width="59" HorizontalContentAlignment="Right" />
                                <TextBlock x:Name="txbStepCC" TextWrapping="Wrap" Text="00" Width="22" Foreground="White" FontFamily="MS Gothic" FontSize="14" TextAlignment="Center" Margin="0,18,32,336" Grid.Column="2" HorizontalAlignment="Right"/>
                                <Label x:Name="LblMethod"
                                       Content="{Binding Resources.LblMethod, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"                                              
                                       FontSize="12" Foreground="White" Height="23" HorizontalAlignment="Left" Margin="2,134,0,0"  VerticalAlignment="Top" Grid.Column="2" Width="46" />
                                <Label x:Name="lblCurrentnDifficultyLevel" Content="difficulty:999" Grid.Column="1" HorizontalAlignment="Right" Margin="0,23,16,0" VerticalAlignment="Top" Foreground="White" Panel.ZIndex="1" FontFamily="MS Gothic" Height="22" Width="94"/>
                                <ScrollViewer HorizontalAlignment="Left" Width="366" VerticalScrollBarVisibility="Auto" Margin="0,22,0,232" Grid.Column="1" >
                                    <TextBox Background="#FF0A0A24" Margin="0" x:Name="lblAnalyzerResult" Foreground="#FFF2F2F2" FontFamily="MS Mincho" FontSize="12" HorizontalContentAlignment="Stretch" TextOptions.TextFormattingMode="Display" TextWrapping="WrapWithOverflow" Width="366" HorizontalAlignment="Left" />
                                </ScrollViewer>
                                <ScrollViewer  VerticalScrollBarVisibility="Auto" Grid.Column="1" Margin="1,156,1,5" />

                                <DataGrid x:Name="DGViewMethodCounter" 
                                          Margin="0,154,0,8" HorizontalAlignment="Left"
                                          Width="364" MinWidth="10" MinHeight="14" 
                                          MinRowHeight="14" RowHeight="14" MinColumnWidth="12" 
                                          
                                          Background="#FF0A0A24" Foreground="White" 
                                          RowBackground="#FF0A0A24" 
                                          VerticalGridLinesBrush="#FFAFB7EB" 
                                          HorizontalGridLinesBrush="#FFAFB7EB" 
                                          BorderBrush="#FF4F4F72" 
                                          
                                	      FontFamily="MS Mincho" FontSize="12" FontStretch="Normal"
                                	
                                          Grid.Column="1" 
                                          Grid.IsSharedSizeScope="True" 
                                	      
                                          HeadersVisibility="None" 
                                          ItemsSource="{Binding}" 
                                	
                                          AutoGenerateColumns="False"
                                          CanUserResizeColumns="True" 
                                          CanUserReorderColumns="False" 
                                          CanUserSortColumns="False" 
                                          FrozenColumnCount="2"                              	
                                          IsSynchronizedWithCurrentItem="True"                                 
                                          IsReadOnly="True" 
                                          SelectionMode="Single" >

                                    <DataGrid.Columns>
                                        <DataGridTextColumn FontFamily="MS Gothic" Foreground="White" Binding="{Binding methodName}" Width="5*"/>
                                        <DataGridTextColumn FontFamily="MS Gothic" Foreground="White" Binding="{Binding difficulty}" Width="1*"/>
                                        <DataGridTextColumn FontFamily="MS Gothic" Foreground="White" Binding="{Binding count}" Width="1*"/>
                                    </DataGrid.Columns>
                                    <DataGrid.RowStyle>
                                        <Style TargetType="{x:Type DataGridRow}">
                                            <Style.Triggers>
                                                <Trigger Property="IsSelected" Value="True">
                                                    <Setter Property="Background" Value="#FF0A0A24" />
                                                    <Setter Property="Foreground" Value="White" />
                                                </Trigger>
                                                <Trigger Property="IsFocused" Value="True">
                                                    <Setter Property="Background" Value="#FF0A0A24" />
                                                    <Setter Property="Foreground" Value="White" />
                                                </Trigger>
                                            </Style.Triggers>
                                        </Style>
                                    </DataGrid.RowStyle>
                                </DataGrid>

                                <RepeatButton x:Name="btnSolve" 
                                        Content="{Binding Resources.btnSolve, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"                                              
                                        FontSize="12" Foreground="White" 
    								    Margin="0,37,0,0" VerticalAlignment="Top" Width="90" Height="24" HorizontalAlignment="Left"
    								    Click="btnSolve_Click" Grid.Column="2" Delay="500" Interval="200" AutomationProperties.HelpText="click or  press hold" ToolTip="click or  press hold">
                                    <RepeatButton.Background>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FFC1B5B5" Offset="0" />
                                            <GradientStop Color="#FF161D6C" Offset="0.472" />
                                            <GradientStop Color="Black" Offset="1" />
                                        </LinearGradientBrush>
                                    </RepeatButton.Background>
                                </RepeatButton>
                                <RepeatButton x:Name="btnUndo" Width="90" Height="24" Margin="0,66,0,0" FontSize="12" Foreground="White" 
                                              Content="{Binding Resources.btnUndo, Mode=OneWay, Source={x:Static app:ResourceService.Current}}" 
                                              HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Column="2"
                                              Click="btnMPre_Click"  Delay="500" Interval="200" ToolTip="click or  press hold">
                                    <RepeatButton.Background>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FF7EA17E" Offset="0" />
                                            <GradientStop Color="#FF103810" Offset="1" />
                                            <GradientStop Color="#FF184818" Offset="0.626" />
                                        </LinearGradientBrush>
                                    </RepeatButton.Background>
                                </RepeatButton>
                                <Button x:Name="btnSolveUp" 
                                        Content="{Binding Resources.btnSolveUp, Mode=OneWay, Source={x:Static app:ResourceService.Current}}" 
                                        FontSize="12" Foreground="White" Height="24" 
                                        HorizontalAlignment="Left" Margin="0,110,0,0" VerticalAlignment="Top" 
                                        Width="90" Click="btnSolveUp_Click" Grid.Column="2">
                                    <Button.Background>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FFC1B5B5" Offset="0" />
                                            <GradientStop Color="#FF161D6C" Offset="0.472" />
                                            <GradientStop Color="Black" Offset="1" />
                                        </LinearGradientBrush>
                                    </Button.Background>
                                </Button>

                                <!--
                                <Button x:Name="btnSolveUp2" 
                                    Content="{Binding Resources.btnSolveUp, Mode=OneWay, Source={x:Static app:ResourceService.Current}}" 
                                    FontSize="12" Foreground="White" Height="24" 
                                    HorizontalAlignment="Left" Margin="395,258,0,0" VerticalAlignment="Top" 
                                    Width="90" Click="btnSDKAnalyzerAuto2" Grid.Column="1">
                                    <Button.Background>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FFC1B5B5" Offset="0" />
                                            <GradientStop Color="#FF161D6C" Offset="0.472" />
                                            <GradientStop Color="Black" Offset="1" />
                                        </LinearGradientBrush>
                                    </Button.Background>
                                </Button>
                            -->
                                <Button x:Name="btnInitialState"
                                        Content="{Binding Resources.btnInitialState, Mode=OneWay, Source={x:Static app:ResourceService.Current}}"                                        
                                        FontSize="12" Foreground="White" Height="24" HorizontalAlignment="Left" 
                                        Margin="0,139,0,0"  VerticalAlignment="Top" Width="90" 
                                        Click="btnAnalyzerResetAll_Click" Grid.Column="2">
                                    <Button.Background>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FF7EA17E" Offset="0" />
                                            <GradientStop Color="#FF103810" Offset="1" />
                                            <GradientStop Color="#FF184818" Offset="0.626" />
                                        </LinearGradientBrush>
                                    </Button.Background>
                                </Button>
                                <Label x:Name="Lbl_onAnalyzing"
